[application]

config/name="Monte Carlo BHOP"
config/description="Coolest shit I've ever made"
run/main_scene="res://Environment.tscn"
boot_splash/fullsize=false
config/icon="res://Images/praise up face.png"

[autoload]

Lines3D="*res://3D_Lines.gd"
cfg="*res://cfg.gd"

[display]

